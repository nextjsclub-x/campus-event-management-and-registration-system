{
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 80,
  "tabWidth": 2,
  "parser": "json",
  "bracketSpacing": true,
  "quoteProps": "consistent",
  "arrowParens": "always",
  "jsxBracketSameLine": false,
  "proseWrap": "preserve",
  "endOfLine": "lf",
  "jsxSingleQuote": true,
  "overrides": [
    {
      "files": ".prettierrc",
      "options": {
        "parser": "json",
        "jsxBracketSameLine": false,
        "jsxSingleQuote": true
      }
    }
  ]
}
